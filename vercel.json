{
    "version": 2,
    "builds": [
      {
        "src": "backend/src/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  