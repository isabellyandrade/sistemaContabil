{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.ts"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/index.html"
      }
    ]
  }
  