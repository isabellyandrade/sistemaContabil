{
    "version": 2,
    "builds": [
      {
        "src": "backend/dist/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  